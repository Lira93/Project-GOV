{% extends 'base.html.twig' %}

{% block title %}Hello ApiCityController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>
    {% include 'navbar.html.twig' %}
<div class="example-wrapper">

    <h1>Station name:</h1>



    Lista stacji:
    <ul>
        {% for station in stations %}

            <li>id: {{ station.id }}</li>
            <li>id station: {{ station.stationId }}</li>
            <li>param name: {{ station.param.paramName }}</li>
            <li>formula: {{ station.param.paramFormula }}</li>
            <li>param code: {{ station.param.paramCode }}</li>
            <br>
        {% endfor %}
    </ul>
</div>
{% endblock %}
